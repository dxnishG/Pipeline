{
  "family": "my-task-definition",
  "executionRoleArn": "arn:aws:iam::676206940164:role/ecs-task-execution-role",

  "containerDefinitions": [
    {
      "name": "my-container",
      "image": "sample-repo7/my-image:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
